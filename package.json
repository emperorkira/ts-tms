    "@types/express": "4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/express-serve-static-core": "4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "14.0.22",
